<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="simpleform" extends="struts-default">
		<action name="save"
			class="com.googlecode.simplelife.struts.simpleform.ProductAction">
			<result name="success">/WEB-INF/simpleform/success.jsp</result>
			<result name="input">/WEB-INF/simpleform/form.jsp</result>
		</action>
		<action name="form">
			<result>/WEB-INF/simpleform/form.jsp</result>
		</action>
	</package>
</struts>
